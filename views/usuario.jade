extends layUsuario

block content
  h1= title
  p 
  table.table.table-hover.table-striped.table-bordered
    thead
      tr
        th #
        th ID
        th TIPO
        th NOMBRE
        th NOTA
        th ESTUDIANTE
        th MATERIA
        th DOCENTE
        th ESTADO
        th FECHA
        th Action
    tbody
    - for (var i = 0; i < news.length; ++i) {
       tr
         th(scope='row') #{i+1}
         td #{news[i]['_id']}
         td #{news[i]['Ltipo']}
         td #{news[i]['Lnombre']}
         td #{news[i]['Lnota']}
         td #{news[i]['Lalumno']}
         td #{news[i]['Lmateria']}
         td #{news[i]['Ldocente']}
         td #{news[i]['Lestados']}
         td #{news[i]['fecha']}
         td
          //a(href="users/pdf/"+news[i]['id'])
          a(href="/users/pdf?id="+news[i]['id'])
           | Generate PDF
    - }
